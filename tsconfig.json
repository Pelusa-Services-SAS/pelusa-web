{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@assets/*": [
				"src/assets/*"
			],
			"@utils/*": [
				"src/utils/*"
			],
			"@models/*": [
				"src/models/*"
			],
			"@constants/*": [
				"src/constants/*"
			],
			"@components/*": [
				"src/components/*"
			],
			"@pages/*": [
				"src/pages/*"
			],
			"@redux/*": [
				"src/redux/*"
			],
			"@hooks/*": [
				"src/hooks/*"
			],
			"@services/*": [
				"src/services/*"
			],
			"@sass/*": [
				"src/sass/*"
			]
		},
		"target": "ESNext",
		"useDefineForClassFields": true,
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ESNext"
		],
		"allowJs": false,
		"skipLibCheck": true,
		"esModuleInterop": false,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react-jsx"
	},
	"include": [
		"src"
	],
	"references": [
		{
			"path": "./tsconfig.node.json"
		}
	]
}